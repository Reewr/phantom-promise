    "mocha": "^2.3.4"
  },
  "bugs": {
    "url": "https://github.com/Reewr/promise-phantom/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Reewr/promise-phantom.git"
  },
  "homepage": "https://github.com/Reewr/promise-phantom#readme",
